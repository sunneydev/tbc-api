{
  "name": "tbc-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup src/app.ts --format esm --clean --minify --metafile",
    "dev": "tsup src/app.ts --format esm --watch --sourcemap --clean --onSuccess \"node dist/app.mjs\"",
    "start": "node dist/app.js",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/ --cache --cache-strategy content"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/prompts": "^2.4.1",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "tsup": "^6.5.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "axios": "1.1.3",
    "jose": "^4.11.0",
    "prompts": "^2.4.2"
  }
}
