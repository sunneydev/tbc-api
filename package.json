{
  "name": "tbc-api",
  "version": "0.1.0",
  "description": "A TypeScript SDK for TBC Bank",
  "private": true,
  "main": "dist/tbc.js",
  "types": "dist/tbc.d.ts",
  "files": [
    "dist/tbc.js",
    "dist/tbc.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunneydev/tbc-api.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/sunneydev/tbc-api#readme",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup src/tbc.ts  --clean --dts --minify --metafile",
    "dev": "tsup src/app.ts --format esm --watch --sourcemap --clean --onSuccess \"node dist/app.mjs\"",
    "start": "node dist/app.js",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/ --cache --cache-strategy content"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/prompts": "^2.4.1",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "jose": "^4.11.0",
    "prompts": "^2.4.2",
    "tsup": "^6.5.0"
  }
}
